{
    "workbench.colorTheme": "Default Dark+",
    "workbench.editorAssociations": {
        "*.pdf": "latex-workshop-pdf-hook"
    },
    "workbench.startupEditor": "none",
    "security.workspace.trust.untrustedFiles": "open",
    "latex-workshop.latex.clean.fileTypes": [
        "*.bbl",
        "*.blg",
        "*.idx",
        "*.ind",
        "*.lof",
        "*.lot",
        "*.out",
        "*.toc",
        "*.acn",
        "*.acr",
        "*.alg",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.log",
        "*.fdb_latexmk",
        "*.snm",
        "*.nav",
        "*.vrb",
        "*.xdv",
        "*.auxlock",
        "*.turtle",
        "*.aux"
    ],
    "editor.accessibilitySupport": "off",
    "latex-workshop.latex.recipes": [
        {
            "name": "lualatex",
            "tools": [
                "lualatex"
            ]
        },
        {
            "name": "lualatex x2",
            "tools": [
                "lualatex",
                "lualatex"
            ]
        },
                {
            "name": "lualatex x3",
            "tools": [
                "lualatex",
                "lualatex",
                "lualatex"
            ]
        },
        {
            "name": "pdflatex",
            "tools": [
                "pdflatex"
            ]
        },
        {
          "name": "pdflatex x2",
          "tools": [
              "pdflatex",
              "pdflatex"
          ]
      },
        {
            "name": "xelatex",
            "tools": [
                "xelatex"
            ]
        },
        {
          "name": "xelatex x2",
          "tools": [
              "xelatex",
              "xelatex"
          ]
      }
    ],
    "security.workspace.trust.enabled": false,
    "latex-workshop.latex.tools": [
        {
            "name": "lualatexmk",
            "command": "latexmk",
            "args": [
                "--shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-lualatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "lualatex",
            "command": "lualatex",
            "args": [
                "--shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "--shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "xelatexmk",
            "command": "latexmk",
            "args": [
                "--shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk_rconly",
            "command": "latexmk",
            "args": [
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "rnw2tex",
            "command": "Rscript",
            "args": [
                "-e",
                "knitr::opts_knit$set(concordance = TRUE); knitr::knit('%DOCFILE_EXT%')"
            ],
            "env": {}
        },
        {
            "name": "jnw2tex",
            "command": "julia",
            "args": [
                "-e",
                "using Weave; weave(\"%DOC_EXT%\", doctype=\"tex\")"
            ],
            "env": {}
        },
        {
            "name": "jnw2texminted",
            "command": "julia",
            "args": [
                "-e",
                "using Weave; weave(\"%DOC_EXT%\", doctype=\"texminted\")"
            ],
            "env": {}
        },
        {
            "name": "pnw2tex",
            "command": "pweave",
            "args": [
                "-f",
                "tex",
                "%DOC_EXT%"
            ],
            "env": {}
        },
        {
            "name": "pnw2texminted",
            "command": "pweave",
            "args": [
                "-f",
                "texminted",
                "%DOC_EXT%"
            ],
            "env": {}
        },
        {
            "name": "tectonic",
            "command": "tectonic",
            "args": [
                "--synctex",
                "--keep-logs",
                "%DOC%.tex"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.message.information.show": true,
    "latex-workshop.view.pdf.external.synctex.args": [
        "-forward-search",
        "%TEX%",
        "%LINE%",
        "-reuse-instance",
        "-inverse-search",
        "%PDF%"
    ],
    "editor.wordWrap": "on",
    "editor.tabSize": 2,
    "latex-workshop.latex.clean.method": "glob",
    "latex-workshop.intellisense.package.enabled": false,
    "latex-workshop.intellisense.package.env.enabled": false,
    "latex-workshop.intellisense.argumentHint.enabled": false,
    "editor.snippets.codeActions.enabled": false,
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "latex-workshop.latex.rootFile.doNotPrompt": true,
    "telemetry.telemetryLevel": "off",
    "editor.formatOnType": false,
    "latex-workshop.latex.autoBuild.cleanAndRetry.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        "*.sty": "latex",
        "*.cls": "latex"
    },
    "editor.autoIndent": "full",
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "editor.fontLigatures": false
}